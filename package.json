  },
  "bugs": {
    "url": "https://github.com/vangware/window-open-promise/issues"
  },
  "dependencies": {
    "@vangware/utils": "^4.0.0"
  },
  "devDependencies": {
    "@types/prettier": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "@vangware/base": "^2.0.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-ban": "^1.5.1",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.2"
  },
  "files": [
    "cjs",
    "esm"
  ],
  "homepage": "https://github.com/vangware/window-open-promise#readme",
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint"
    }
  },
  "keywords": [
    "front",
    "front-end",
    "javascript",
    "open",
    "promise",
    "promised",
    "window"
  ],
  "license": "MIT",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vangware/window-open-promise.git"
  },
  "scripts": {
    "clean": "rimraf ./{cjs,esm}",
    "compile": "run-s compile:*",
    "compile:cjs": "tsc --project tsconfig.json",
    "compile:esm": "tsc --project tsconfig.esm.json",
    "lint": "eslint src/*.ts",
    "prepublishOnly": "run-s compile prettify",
    "prettify": "run-s prettify:*",
    "prettify:cjs": "prettier --write './cjs/**/*.{js,ts}'",
    "prettify:esm": "prettier --write './esm/**/*.{js,ts}'",
    "prettify:esm-fix": "prettify-esm ./esm",
    "test": "echo 'No test yet, sorry'"
  },
  "source": "./src/index.ts",
  "types": "./esm/index.d.ts"
}
