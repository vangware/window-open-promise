{
	"name": "@vangware/window-open-promise",
	"description": "ðŸªŸ Promised Window.open();",
	"version": "4.1.5",
	"author": {
		"name": "Vangware",
		"url": "https://vangware.com"
	},
	"bugs": {
		"url": "https://github.com/vangware/window-open-promise/issues"
	},
	"dependencies": {
		"@vangware/utils": "^6.3.4"
	},
	"devDependencies": {
		"@types/prettier": "^2.3.0",
		"@typescript-eslint/eslint-plugin": "^4.28.0",
		"@typescript-eslint/parser": "^4.28.0",
		"@vangware/base": "^4.7.7",
		"@vangware/test": "^6.0.3",
		"c8": "^7.7.3",
		"eslint": "^7.29.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-import-resolver-node": "^0.3.4",
		"eslint-plugin-ban": "^1.5.2",
		"eslint-plugin-functional": "^3.2.1",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-no-null": "^1.0.2",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "^3.4.0",
		"husky": "4.3.8",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.3.2",
		"rimraf": "^3.0.2",
		"typedoc": "^0.21.1",
		"typescript": "^4.3.4"
	},
	"exports": {
		".": {
			"import": "./esm/index.js",
			"require": "./cjs/index.js"
		}
	},
	"files": [
		"cjs/**/*",
		"esm/**/*"
	],
	"homepage": "https://window-open-promise.vangware.com",
	"husky": {
		"hooks": {
			"pre-commit": "run-s clean lint pre-compile test docs"
		}
	},
	"keywords": [
		"front",
		"front-end",
		"javascript",
		"open",
		"promise",
		"promised",
		"window"
	],
	"license": "MIT",
	"main": "./cjs/index.js",
	"module": "./esm/index.js",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/vangware/window-open-promise.git"
	},
	"scripts": {
		"clean": "rimraf ./{cjs,esm}",
		"compile": "run-s compile:*",
		"compile:cjs": "tsc --project tsconfig.cjs.json",
		"compile:esm": "tsc --project tsconfig.esm.json",
		"docs": "typedoc --disableOutputCheck --gitRevision main --theme minimal ./src/index.ts",
		"lint": "eslint {src,tests}/**/*.ts",
		"lint:fix": "eslint {src,tests}/**/*.ts --fix",
		"postpublish": "run-s clean",
		"pre-compile": "run-s pre-compile:*",
		"pre-compile:cjs": "tsc --noemit --project tsconfig.cjs.json",
		"pre-compile:esm": "tsc --noemit --project tsconfig.esm.json",
		"prepublishOnly": "run-s compile prettify",
		"prettify": "prettier --write --loglevel warn './{cjs,esm}/**/*.{js,ts}'",
		"test": "c8 --reporter=html --reporter=text-summary --reporter=lcov test"
	},
	"source": "./src/index.ts",
	"types": "./esm/index.d.ts"
}
