{
  "name": "@vangware/window-open-promise",
  "description": "Promised Window.open",
  "version": "4.0.1",
  "author": {
    "name": "Vangware",
    "url": "https://vangware.com"
  },
  "bugs": {
    "url": "https://github.com/vangware/window-open-promise/issues"
  },
  "dependencies": {
    "@vangware/utils": "^5.0.4"
  },
  "devDependencies": {
    "@types/prettier": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@vangware/base": "^2.2.8",
    "@vangware/test": "^4.0.4",
    "c8": "^7.3.4",
    "coveralls": "^3.1.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-ban": "^1.5.1",
    "eslint-plugin-functional": "^3.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "files": [
    "cjs",
    "esm"
  ],
  "homepage": "https://window-open-promise.vangware.com",
  "husky": {
    "hooks": {
      "pre-commit": "run-s clean lint pre-compile test docs"
    }
  },
  "keywords": [
    "front",
    "front-end",
    "javascript",
    "open",
    "promise",
    "promised",
    "window"
  ],
  "license": "MIT",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vangware/window-open-promise.git"
  },
  "scripts": {
    "clean": "rimraf ./{cjs,esm}",
    "compile": "run-s compile:*",
    "compile:cjs": "tsc --project tsconfig.cjs.json",
    "compile:esm": "tsc --project tsconfig.esm.json",
    "coverage": "c8 report --reporter=text-lcov | coveralls",
    "docs": "typedoc --gitRevision main --mode file --out ./docs ./src/index.ts",
    "lint": "eslint src/*.ts",
    "pre-compile": "run-s pre-compile:*",
    "pre-compile:cjs": "tsc --noemit --project tsconfig.cjs.json",
    "pre-compile:esm": "tsc --noemit --project tsconfig.esm.json",
    "prepublishOnly": "run-s compile prettify",
    "prettify": "run-s prettify:*",
    "prettify:cjs": "prettier --write './cjs/**/*.{js,ts}'",
    "prettify:esm": "prettier --write './esm/**/*.{js,ts}'",
    "prettify:esm-fix": "prettify-esm ./esm",
    "test": "c8 --reporter=html --reporter=text-summary test"
  },
  "source": "./src/index.ts",
  "types": "./esm/index.d.ts"
}
