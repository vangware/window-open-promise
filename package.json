{
  "name": "@vangware/window-open-promise",
  "description": "Promised Window.open",
  "version": "2.1.1",
  "author": {
    "name": "Vangware",
    "url": "https://vangware.com"
  },
  "bugs": {
    "url": "https://github.com/vangware/window-open-promise/issues"
  },
  "dependencies": {
    "@vangware/micro": "1.1.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/prettier": "1.18.3",
    "@vangware/vangstyle": "1.4.2",
    "husky": "3.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "3.0.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "files": [
    "cjs",
    "esm"
  ],
  "homepage": "https://github.com/vangware/window-open-promise#readme",
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint"
    }
  },
  "keywords": [
    "front",
    "front-end",
    "javascript",
    "open",
    "promise",
    "promised",
    "window"
  ],
  "license": "MIT",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vangware/window-open-promise.git"
  },
  "scripts": {
    "clean": "rimraf ./{cjs,esm}",
    "compile": "run-s compile:*",
    "compile:cjs": "tsc --project tsconfig.json",
    "compile:esm": "tsc --project tsconfig.esm.json",
    "lint": "tslint -c ./tslint.json -p .",
    "prepublishOnly": "run-s compile",
    "prettify": "run-s prettify:*",
    "prettify:cjs": "prettier --write './cjs/**/*.{js,ts}'",
    "prettify:esm": "prettier --write './esm/**/*.{js,ts}'",
    "test": "echo 'No test yet, sorry'"
  },
  "source": "./src/index.ts",
  "types": "./esm/index.d.ts"
}
