{
  "compilerOptions": {
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "listEmittedFiles": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "LF",
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./cjs",
    "preserveConstEnums": true,
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es2015"
  }
}
