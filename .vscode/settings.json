{
	"cSpell.words": [
		"Vangstyle",
		"Vangware",
		"editorconfig",
		"eslintconfig",
		"iife",
		"minimizable",
		"plusplus",
		"pnpm",
		"pnpx",
		"prettierignore",
		"stylelint",
		"stylelintrc",
		"tslib"
	],
	"editor.codeActionsOnSave": {
		"source.organizeImports": "explicit",
		"source.fixAll.eslint": "explicit"
	},
	"editor.formatOnSave": true,
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"typescript.tsdk": "node_modules/typescript/lib"
}
