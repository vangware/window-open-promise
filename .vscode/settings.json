{
  "cSpell.words": [
    "Vangstyle",
    "Vangware",
    "editorconfig",
    "eslintconfig",
    "iife",
    "minimizable",
    "plusplus",
    "pnpm",
    "pnpx",
    "prettierignore",
    "stylelint",
    "stylelintrc",
    "tslib"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "eslint.autoFixOnSave": true,
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.enable": true
}
